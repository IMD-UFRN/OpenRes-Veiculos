<h1>Listing car_reservations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_reservations.each do |car_reservation| %>
      <tr>
        <td><%= link_to 'Show', car_reservation %></td>
        <td><%= link_to 'Edit', edit_car_reservation_path(car_reservation) %></td>
        <td><%= link_to 'Destroy', car_reservation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car reservation', new_car_reservation_path %>
